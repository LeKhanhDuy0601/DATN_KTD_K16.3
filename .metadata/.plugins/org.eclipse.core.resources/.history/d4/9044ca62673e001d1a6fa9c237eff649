package MobileShop.Entity;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
@Entity
@Table(name = "accounts")
public class accounts {
	@Id
	String username;
	String fullname;
	String password;
	String phone;
	String email;
	String birthday;
	String address;
	String avatar;
	boolean deleted;
	
	
}
